<div class="d-grid gap-2">
  <a href="/job/create" class="btn btn-primary"><i class="fa fa-plus" aria-hidden="true"></i> New job</a>
</div>
<table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">schedule</th>
        <th scope="col">actif</th>
      </tr>
    </thead>
    <tbody>
      {{#data}}
      <tr>
        <th scope="row">{{key}}</th>        
        <td>{{value.name}}</td> 
        <td>{{value.schedule}}</td>
        <td>{{value.actif}}</td>
      </tr>
      {{/data}}
    </tbody>
  </table>